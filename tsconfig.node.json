{
  "compilerOptions": {
    "allowJs": true,
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "baseUrl": "./src",
    "paths": {
      "*": [ "*" ]
    },
    "target": "ESNext",
    "lib": [ "DOM", "ESNext" ],
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [ "vite.config.ts" ]
}
